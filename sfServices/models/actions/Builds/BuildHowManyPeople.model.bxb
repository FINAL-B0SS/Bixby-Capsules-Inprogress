action (BuildHowManyPeople) {
  type (Search)
  collect {
    input (serviceInfo) {
      type (ServiceInfo)
      min (Required) max (One)
    }
    input (voiceKey) {
      type (VoicePeople)
      min (Optional) max (One)
    }
    input (valueKey) {
      type (ValueKey)
      min (Required) max (One)
      default-init {
        intent {
          goal: ValueKey
          value: ValueKey(how many people?)
        }
      }
    }
  }
  output (HowManyPeople)
}
