action (BuildObject) {
  type (Constructor)
  collect {
    input (serviceInfo) {
      type (ServiceInfo)
      min (Required) max (One)
    }
    input (voiceKey) {
      type (VoiceObject)
      min (Optional) max (One)
    }
    input (valueKey) {
      type (ValueKey)
      min (Required) max (One)
      default-init {
        intent {
          goal: ValueKey
          value: ValueKey(object)
        }
      }
    }
  }
  output (Object)
}
