action (BuildWholeBlock) {
  type (Constructor)
  collect {
    input (serviceInfo) {
      type (ServiceInfo)
      min (Required) max (One)
    }
    input (voiceKey) {
      type (VoiceWholeBlock)
      min (Optional) max (One)
    }
    input (valueKey) {
      type (ValueKey)
      min (Required) max (One)
      default-init {
        intent {
          goal: ValueKey
          value: ValueKey(whole block?)
        }
      }
    }
  }
  output (WholeBlock)
}
