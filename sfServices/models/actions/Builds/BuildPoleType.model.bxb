action (BuildPoleType) {
  type (Constructor)
  collect {
    input (serviceInfo) {
      type (ServiceInfo)
      min (Required) max (One)
    }
    input (voiceKey) {
      type (VoicePoleType)
      min (Optional) max (One)
    }
    input (valueKey) {
      type (ValueKey)
      min (Required) max (One)
      default-init {
        intent {
          goal: ValueKey
          value: ValueKey(pole type)
        }
      }
    }
  }
  output (PoleType)
}
