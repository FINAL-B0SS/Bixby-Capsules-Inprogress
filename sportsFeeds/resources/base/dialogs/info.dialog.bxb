dialog (Result) {
  description (What's shown at the top of the page when users are browsing a RSS feed)
  match: Info (this)
  if (size(this) > 1 && exists(this.tag)) {
    choose (Random) {
    template("#{value(this.tag)} headlines")
    template ("#{value(this.tag)} stories")
    template ("#{value(this.tag)} news")
    }
  } else-if (exists(this.description)){
    template () {
      speech ("#{value(this.description)}")
    }
  }
}