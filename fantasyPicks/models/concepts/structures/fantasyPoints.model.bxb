structure (FantasyPoints) {
  description (player fantasy points info)
  property ("seasonTotal") {
    type (PointsSeasonTotal)
    min (Required) max (One)
  }
  property ("avgPerGame") {
    type (PointsAvg)
    min (Required) max (One)
  }
  property ("lastWeek") {
    type (PointsLastWeek)
    min (Required) max (One)
  }
}
