  targets {
    target (bixby-mobile-en)
  }
  permissions{
    lock-screen-enabled
  }
  store-sections {
    section (ArtAndLifestyle)
  }
  capsule-imports {
    import (viv.core) {
      as (core)
    }
    import (viv.image) {
      as (image) version (2.12.0)
    }
  }
}
